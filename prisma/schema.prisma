generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id        Int       @id @default(autoincrement())
    email     String    @unique
    password  String
    firstName String
    lastName  String
    birthday  DateTime? @db.Date
    createdAt DateTime  @default(now())
}
